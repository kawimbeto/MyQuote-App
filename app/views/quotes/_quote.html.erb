<div id="<%= dom_id quote %>">
  <p>
    <strong>Body:</strong>
    <%= quote.body %>
  </p>

  <p>
    <strong>Publication:</strong>
    <%= quote.publication %>
  </p>

  <p>
    <strong>Comment:</strong>
    <%= quote.comment %>
  </p>

  <p>
    <strong>Is public:</strong>
    <%= quote.is_public %>
  </p>

  <p>
    <strong>User:</strong>
    <%= quote.user_id %>
  </p>

  <p>
    <strong>Philosopher:</strong>
    <%= quote.philosopher_id %>
  </p>

  <p>
    <strong>Category:</strong>
    <%= quote.category_id %>
  </p>

</div>
